<!-- Log in view to create new session.-->
<div class="row">
  <div class="small-3 small-centered columns">
    <h1>Log in</h1>
    <br/>
    <%= form_tag sessions_path do %>
        <%= label_tag :email %>
        <%= text_field_tag :email, params[:email] %>
        <%= label_tag :password %>
        <%= password_field_tag :password %>
        <%= submit_tag "Log in", :class => "button tiny" %>
    <% end %>
  </div>
</div>